{
  "name": "@marceloemanoel/alphavantage",
  "version": "1.0.0",
  "description": "A simple interface to the Alpha Vantage API written in TypeScript.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
