  "bugs": {
    "url": "https://github.com/marceloemanoel/alphavantage"
  },
  "homepage": "https://github.com/marceloemanoel/alphavantage",
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "cross-fetch": "^3.0.4"
  },
  "devDependencies": {
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.4",
    "coveralls": "^3.0.6",
    "dotenv": "^8.1.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typedoc": "^0.15.6",
    "typescript": "^3.7.4"
  }
